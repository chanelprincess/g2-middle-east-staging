{
  "version": 2,
  "name": "g2-middle-east-staging",
  "projectSettings": {
    "framework": "nextjs",
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "outputDirectory": ".next"
  },
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_SITE_URL": "https://g2-staging.vercel.app"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_SUPABASE_URL": "@supabase-url-staging",
      "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@supabase-anon-key-staging",
      "SUPABASE_SERVICE_ROLE_KEY": "@supabase-service-key-staging",
      "OPENAI_API_KEY": "@openai-key-staging"
    }
  }
}
